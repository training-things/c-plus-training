  // "terminal.integrated.shell.windows": "cmd.exe",
  "commentTranslate.targetLanguage": "zh-CN",
  "code-runner.executorMap": {
    // "c": "cd $dir && gcc -Wall -Wextra -std=c99 -g $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ -Wall -Wextra -std=c++11 -O2 -I./include -lpthread -g $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "googletest.failed",
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "scope": "googletest.passed",
        "settings": {
          "foreground": "#0f0"
        }
      },
      {
        "scope": "googletest.run",
        "settings": {
          "foreground": "#0f0"
        }
      }
    ]
  },
  "gtest-adapter.debugConfig": ["clang++ - Build and debug active file"],
  "cmake.configureSettings": { "gtest_build_samples": "ON" },
  // google test debug
  "testMate.cpp.debug.configTemplate": {
    "type": "cppdbg",
    "MIMode": "lldb",
    "program": "${exec}",
    "args": "${argsArray}",
    "cwd": "${cwd}",
    "env": "${envObj}",
    "sourceFileMap": "${sourceFileMapObj}",
    "externalConsole": false
  },
  "C_Cpp.errorSquiggles": "disabled"
}
